@{
    ViewData["Title"] = "_UILayout";
}
<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_HeadLayoutComponentPartial")

<body>
    <!-- Topbar Start -->
    @await Component.InvokeAsync("_TopBarLayoutComponentPartial")
    <!-- Topbar End -->
    <!-- Navbar Start -->
    @await Component.InvokeAsync("_NavBarLayoutComponentPartial")
    <!-- Navbar End -->
    <!--breadcrumb Start-->
    @await Component.InvokeAsync("_BreadcrumbUrlLayoutComponentPartial")
    <!--breadcrumb End-->
    @RenderBody()

    <!-- Footer Start -->
    @await Component.InvokeAsync("_FooterLayoutComponentPartial")
    <!-- Footer End -->
    <!-- Back to Top -->
    <a href="#" class="btn btn-primary back-to-top"><i class="fa fa-angle-double-up"></i></a>

    @await Component.InvokeAsync("_ScriptLayoutComponentPartial")
    @await Component.InvokeAsync("NToastNotify")
</body>
</html>

