@model List<ResultCargoCustomerDto>

@{
    ViewData["Title"] = "UserAddressInfo";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>Kullanıcı Adres Listesi</h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-home"></i> </a></li>
        <li class="active"><i class="fa fa-bullseye"></i> Kullanıcılar</li>
        <li class="active"><i class="fa fa-envelope"></i> Adres Listesi</li>
    </ol>
</section>

<!-- Main content -->
<section class="content container-fluid">
    <div class="row">

        @foreach (var item in Model)
        {
            <div class="col-md-6">
                <div class="chart-box">
                    <div class="row">
                        <div class="col-md-3">
                            <div class="user-img pull-left">
                                <img src="~/Admin/dist/img/img3.jpg" class="img-circle img-responsive" alt="User Image">
                            </div>
                        </div>
                        <div class="col-md-9">
                            <div class="mail-contnet">
                                <h4>@item.CustomerTitle <span class="desig">- @item.CustomerName @item.CustomerSurname</span></h4>
                                <p class="m-bot-2">@item.CustomerAddress - @item.CustomerCity - @item.CustomerDistrict</p>
                                <h5>Telefon:@item.CustomerPhone </h5>
                                <h5>Email:@item.CustomerEmail </h5>
                                <ul class="social-icons">
                                    <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                                    <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                    <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                                    <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                                    <li><a href="#"><i class="fa fa-flickr"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }


    </div>
</section>
<!-- content -->